{
  "name": "rbush",
  "version": "3.0.1",
  "description": "High-performance 2D spatial index for rectangles (based on R*-tree with bulk loading and bulk insertion algorithms)",
  "homepage": "https://github.com/mourner/rbush",
  "repository": {
    "type": "git",
    "url": "git://github.com/mourner/rbush.git"
  },
  "keywords": [
    "spatial",
    "tree",
    "search",
    "rectangle",
    "index",
    "math"
  ],
  "author": "Vladimir Agafonkin",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "devDependencies": {
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "benchmark": "^2.1.4",
    "c8": "^10.0.0",
    "eslint": "^9.4.0",
    "eslint-config-mourner": "^3.0.0",
    "esm": "^3.2.25",
    "rollup": "^4.18.0",
    "tape": "^5.7.5"
  },
  "scripts": {
    "pretest": "eslint index.js test/test.js bench/*.js",
    "fix": "eslint --fix index.js test/test.js bench/*.js",
    "test": "tsc && node test/test.js",
    "perf": "node ./bench/perf.js",
    "cov": "c8 npm run test",
    "build": "rollup -c",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "index.js"
  ],
  "eslintConfig": {
    "extends": [
      "mourner"
    ]
  },
  "dependencies": {
    "quickselect": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
